<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>DeptAddServlet</servlet-name>
    <servlet-class>com.zwl.controller.DeptAddServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeptFindServlet</servlet-name>
    <servlet-class>com.zwl.controller.DeptFindServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteDeptServlet</servlet-name>
    <servlet-class>com.zwl.controller.DeleteDeptServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SelectOneServlet</servlet-name>
    <servlet-class>com.zwl.controller.SelectOneServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeptAddServlet</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeptFindServlet</servlet-name>
    <url-pattern>/find</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteDeptServlet</servlet-name>
    <url-pattern>/deleteDept</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectOneServlet</servlet-name>
    <url-pattern>/selectOne</url-pattern>
  </servlet-mapping>
</web-app>
